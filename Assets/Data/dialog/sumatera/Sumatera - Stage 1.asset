  m_Script: {fileID: 11500000, guid: f99e24c948d4594478af21e268678eab, type: 3}
  m_Name: Sumatera - Stage 1
  m_EditorClassIdentifier: Assembly-CSharp::NinuNinu.Systems.DialogueData
  lines:
  - characterName: Local Residents
    side: 1
    text: '"Look at this river... we used to be able to drink straight from here,
      now it''s full of rubbish and chemical waste."'
  - characterName: Ranger Player
    side: 0
    text: '"The condition is quite serious. What should I do to help?"'
  - characterName: Local Residents
    side: 1
    text: '"You have to pick up the plastic trash in the water and fix the leaking
      sewage pipes there."'
  - characterName: Local Residents
    side: 1
    text: '"Wait, listen to my advice: fix the Water Filter Machine first. It''ll
      help slow down the pollution."

'
  - characterName: Ranger Player
    side: 0
    text: '"Understood! The water filter is the first priority, then clean all the
      trash and pipes. I''ll start now!"'
